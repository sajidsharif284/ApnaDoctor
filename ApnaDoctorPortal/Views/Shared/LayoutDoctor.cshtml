                <div class="profile-social-dtl">
                    <ul class="dtl-social">
                        <li><a href="@Url.Action("DetailDoctorProfile","Doctor")" ><i class="icon nalika-user icon-wrap"></i></a></li>
                        <li><a href="@Url.Action("Index","Doctor")"><i class="icon nalika-home icon-wrap"></i></a></li>
                        <li><a href="@Url.Action("RegisterPatient","Account")"><i class="icon nalika-diamond icon-wrap"></i></a></li>
                    </ul>
                </div>
            </div>
            <div class="left-custom-menu-adp-wrap  comment-scrollbar">
                <nav class="sidebar-nav left-sidebar-menu-pro ">
                    <ul class="metismenu " id="menu1">
                        <li>
